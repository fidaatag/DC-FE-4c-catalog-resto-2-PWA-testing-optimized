{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jasmine": true,
        "codeceptjs/codeceptjs": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "new-cap": 0,
        "valid-jsdoc":"off",
        "require-jsdoc":"off",
        "max-len":"off",
        "no-unused-vars":"off"
    },
    "plugins": [
        "jasmine",
        "codeceptjs"
    ]
}
